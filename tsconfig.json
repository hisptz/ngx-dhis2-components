{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "ngx-dhis2-dictionary": [
        "dist/ngx-dhis2-dictionary/ngx-dhis2-dictionary",
        "dist/ngx-dhis2-dictionary"
      ],
      "ngx-dhis2-http-client": [
        "dist/ngx-dhis2-http-client/ngx-dhis2-http-client",
        "dist/ngx-dhis2-http-client"
      ],
      "ngx-dhis2-org-unit-filter": [
        "dist/ngx-dhis2-org-unit-filter/ngx-dhis2-org-unit-filter",
        "dist/ngx-dhis2-org-unit-filter"
      ],
      "ngx-dhis2-menu": [
        "dist/ngx-dhis2-menu/ngx-dhis2-menu",
        "dist/ngx-dhis2-menu"
      ],
      "ngx-dhis2-period-filter": [
        "dist/ngx-dhis2-period-filter/ngx-dhis2-period-filter",
        "dist/ngx-dhis2-period-filter"
      ],
      "ngx-maps": ["dist/ngx-maps/ngx-maps", "dist/ngx-maps"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
